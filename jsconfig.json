{
	"compilerOptions": {
		"target": "ES2021",
		"module": "ESNext",
		"checkJs": true,
		"strict": false,
		"noImplicitAny": false,
		"strictNullChecks": false,
		"strictPropertyInitialization": false,
		"skipLibCheck": true,
		"lib": [
			"ESNext",
			"DOM",
			"DOM.Iterable"
		],
		"noEmit": true
	},
	"include": [
		"src/**/*.js"
	],
	"typeAcquisition": {
		"enable": true
	}
}
