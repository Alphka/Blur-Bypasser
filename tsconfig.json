{
	"compilerOptions": {
		"target": "ES2021",
		"module": "ESNext",
		"moduleResolution": "Node",
		"strict": true,
		"sourceMap": true,
		"noImplicitAny": false,
		"skipLibCheck": true,
		"lib": [
			"ESNext",
			"DOM",
			"DOM.Iterable"
		],
		"noEmit": true
	},
	"ignore": [
		"node_modules",
		"build"
	]
}
